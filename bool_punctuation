def get_punctuation(message):
    if not message:
        return "Empty string"

    last_character = message[-1]

    if last_character == "?":
        return "question"
    elif last_character == ".":
        return "statement"
    else:
        return "other"

print(get_punctuation(""))
message = "This is a question?"
print(get_punctuation(message))
print(get_punctuation("This is not a question."))
print(get_punctuation("This is CC101"))

    